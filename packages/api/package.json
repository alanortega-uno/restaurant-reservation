{
  "name": "@restaurant-reservation/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.js",
    "build": "npx tsc -p tsconfig.json",
    "start": "node dist/api/src/index.js",
    "dev": "nodemon src/index.ts",
    "e2e:dev": "NODE_ENV=e2e nodemon src/index.ts",
    "typeorm:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/db/dataSourceLocal.ts",
    "typeorm:migrate": "npx typeorm-ts-node-commonjs migration:run -d src/db/dataSourceLocal.ts",
    "typeorm:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/db/dataSourceLocal.ts",
    "e2e:typeorm:migrate": "NODE_ENV=e2e npx typeorm-ts-node-commonjs migration:run -d src/db/dataSourceLocal.ts",
    "e2e:typeorm:revert": "NODE_ENV=e2e npx typeorm-ts-node-commonjs migration:revert -d src/db/dataSourceLocal.ts",
    "e2e:reset:db": "npm run e2e:typeorm:revert && npm run e2e:typeorm:migrate",
    "typeorm:drop": "npx typeorm-ts-node-commonjs schema:drop -d src/db/dataSourceLocal.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
